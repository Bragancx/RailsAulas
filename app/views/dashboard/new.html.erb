<h1>Dashboard#new</h1>
<%= form_with(url: "/dashboard", method: :post, local: true, model: @user) do |f| %>
    <label for="name">Name</label>
    <%= f.text_field :name, id: "name" %>
    <br>
    <label for="age">Age</label>
    <%= f.number_field :age, id: "age" %>

    <%= f.submit "Save" %>
<% end %>